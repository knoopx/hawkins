{
  "rules": {
    ".read": true,
    ".write": true,
    "pushes": {
      "$item": {
        ".validate": "newData.hasChildren() && newData.child('pusher').exists() && newData.child('deleted').val() === false && !newData.child('head_commit').child('message').val().contains('[ci skip]')"
      }
    }
  }
}
